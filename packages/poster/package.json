  "license": "ISC",
  "files": [
    "dist",
    "README.md",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "squint-cljs": "latest"
  },
  "type": "module"
}
